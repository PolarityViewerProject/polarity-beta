<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<floater
  bevel_style="none"
  border_style="line"
  can_resize="true"
  height="560"
  layout="topleft"
  min_height="271"
  min_width="575"
  name="script ed float"
  help_topic="script_ed_float"
  save_rect="true"
  title="[S] New Script"
  width="508">
  <floater.string
    name="not_allowed">
    You can not view or edit this script, since it has been set as &quot;no copy&quot;. You need full permissions to view or edit a script inside an object.
  </floater.string>
  <floater.string
    name="script_running">
    Running
  </floater.string>
  <floater.string
    name="Title">
    [NAME]
  </floater.string>
  <floater.string
    name="experience_enabled">
    Uncheck to remove the current experience
  </floater.string>
  <floater.string
    name="no_experiences">
    You are not authorized for any experiences
  </floater.string>
  <floater.string
    name="add_experiences">
    Select to add an experience
  </floater.string>
  <floater.string
    name="show_experience_profile">
    Click to view the experience profile
  </floater.string>
  <floater.string
    name="loading">
    Loading...
  </floater.string>
  <panel
    bevel_style="none"
     
    border_style="line"
    follows="left|top|right|bottom"
    height="552"
    layout="topleft"
    left="5"
    name="script ed panel"
    top="0"
    width="497">
  </panel>
  <button
    follows="left|bottom"
    height="31"
    label="Reset"
    label_selected="Reset"
    layout="topleft"
    name="Reset"
    left="4"
    top_pad="-26"
    width="55">
  </button>
  <check_box
    enabled="false"
    follows="left|bottom"
    font="SansSerif"
    height="18"
    initial_value="true"
    label="Mono"
    layout="topleft"
    left_pad="0"
    name="mono"
    top_delta="-1"
    width="70">
  </check_box>
  <check_box
    enabled="true"
    follows="left|bottom"
    font="SansSerif"
    height="18"
    initial_value="true"
    label="Running"
    layout="topleft"
    left_delta="0"
    name="running"
    top_pad="-3"
    width="65">
  </check_box>
  <check_box
             enabled="false"
    follows="left|bottom"
    height="18"
    label="Experience"
             layout="topleft"
    left_pad="5"
    name="enable_xp"
    top_delta="0"
    width="90">
  </check_box>
  <combo_box
    label="Experiences..."
    top_delta="-14"
    height="31"
    left_pad="0"
    right="379"
    layout="topleft"
    follows="left|bottom|right"
    visible="true"
    name="Experiences...">
    <combo_box.item
      label="Fegit cat experience"
      name="asdfmovie"
      value="fegit">
    </combo_box.item>
  </combo_box>
  <button label="&gt;"
          name="view_profile"
    height="31"
          width="23"
    right="402"
          layout="topleft"
    top_delta="0"
          follows="right"
    visible="true">
  </button>
</floater>
